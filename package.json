{
  "name": "@blockingmachine/electron-app",
  "productName": "Blockingmachine (Beta)",
  "version": "1.0.0-beta.3",
  "description": "An application to generate filter lists for Adguard Home and more.",
  "license": "BSD-3-Clause",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:native": "electron-forge make --platform=darwin",
    "build": "npm run make:native",
    "make:win": "electron-forge make --platform=win32",
    "make:mac": "electron-forge make --platform=darwin",
    "release": "electron-forge make --platform=darwin && electron-builder --publish always"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-dmg": "^7.8.0",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "@electron-forge/plugin-fuses": "^6.4.2",
    "@electron-forge/plugin-webpack": "^7.8.0",
    "@electron-forge/shared-types": "^6.4.2",
    "@electron/typescript-definitions": "^9.1.2",
    "@types/node": "^20.17.32",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@vercel/webpack-asset-relocator-loader": "^1.7.4",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "electron": "^29.4.6",
    "esbuild-loader": "^4.3.0",
    "node-loader": "^2.1.0",
    "style-loader": "^4.0.0",
    "ts-node": "11.0.0-beta.1",
    "typescript": "^5.8.3",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@blockingmachine/core": "1.0.0-beta.3",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.3.4",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.15.3"
  },
  "build": {
    "appId": "com.blockingmachine.app",
    "publish": {
      "provider": "github",
      "owner": "greigh",
      "repo": "Blockingmachine"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/greigh/Blockingmachine.git"
  },
  "homepage": "https://github.com/greigh/Blockingmachine",
  "bugs": {
    "url": "https://github.com/greigh/Blockingmachine/issues"
  },
  "author": {
    "name": "Daniel Hipskind",
    "email": "me@danielhipskind.com",
    "url": "https://danielhipskind.com"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "typeRoots": [
    "./node_modules/@types",
    "./src/types"
  ]
}
